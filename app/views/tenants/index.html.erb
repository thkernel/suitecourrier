<p id="notice"><%= notice %></p>

<h1>Tenants</h1>

<table>
  <thead>
    <tr>
      <th>Organization type</th>
      <th>Organization name</th>
      <th>Address</th>
      <th>Phone</th>
      <th>City</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Website</th>
      <th>Subdomain</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tenants.each do |tenant| %>
      <tr>
        <td><%= tenant.organization_type %></td>
        <td><%= tenant.organization_name %></td>
        <td><%= tenant.address %></td>
        <td><%= tenant.phone %></td>
        <td><%= tenant.city %></td>
        <td><%= tenant.phone %></td>
        <td><%= tenant.email %></td>
        <td><%= tenant.website %></td>
        <td><%= tenant.subdomain %></td>
        <td><%= tenant.status %></td>
        <td><%= link_to 'Show', tenant %></td>
        <td><%= link_to 'Edit', edit_tenant_path(tenant) %></td>
        <td><%= link_to 'Destroy', tenant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tenant', new_tenant_path %>
