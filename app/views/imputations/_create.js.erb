$("ul.errors").html("");
<% if @imputation.errors.any? %>
  <% @imputation.errors.full_messages.each do |message| %>
 
    $("ul.errors").append($('<div class="c-alert c-alert--danger" > ').html("<%= message.html_safe %> "));
  <% end %>
<% else %>
  $(".imputation-items").html("<%= escape_javascript(render 'imputations/partials/index') %>");
  $("#imputation-modal").modal("hide");
 
<% end %>